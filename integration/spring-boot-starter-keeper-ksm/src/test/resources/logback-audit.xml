<configuration>
  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>build/audit.log</file>
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>
  <logger name="com.keepersecurity" level="INFO" additivity="false">
    <appender-ref ref="FILE"/>
  </logger>
</configuration>
